    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-surveyjs": "file:eslint-surveyjs",
    "eslint-plugin-vue": "^8.3.0",
    "file-loader": "6.2.0",
    "generate-json-webpack-plugin": "^2.0.0",
    "github-api": "^3.4.0",
